/*Escreva um algoritmo que leia dois números inteiros e os apresente em ordem crescente */

#include <stdio.h>

int main (){
	
	int num1, num2;
	
	printf("Informe um numero inteiro: ");
	scanf("%d", &num1);
	
	printf("Informe um numero inteiro: ");
	scanf("%d", &num2);
	
	
	if (num1 < num2){
		printf("O número em ordem crescente é: %d e %d", num1, num2 );
	}
	
	else{
		printf("O número em ordem crescente é: %d e %d", num2, num1);
	}
	
	return 0;
}
